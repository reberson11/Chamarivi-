.github/workflows/build-apk.yml
name: Build APK Chamarivi

on:
  workflow_dispatch:

jobs:
  build:
    runs-on: ubuntu-latest

    steps:
      - uses: actions/checkout@v4

      - name: Set up Java
        uses: actions/setup-java@v4
        with:
          distribution: 'temurin'
          java-version: '17'

      - name: Set up Flutter
        uses: subosito/flutter-action@v2
        with:
          flutter-version: '3.19.6'
          channel: 'stable'

      - name: Flutter pub get
        run: flutter pub get
        working-directory: chamarivi

      - name: Build APK
        run: flutter build apk --release
        working-directory: chamarivi

      - name: Upload APK
        uses: actions/upload-artifact@v4
        with:
          name: chamarivi-apk
          path: chamarivi/build/app/outputs/flutter-apk/app-release.apk

